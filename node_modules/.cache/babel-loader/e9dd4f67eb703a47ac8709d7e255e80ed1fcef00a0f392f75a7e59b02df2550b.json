{"ast":null,"code":"import { Line } from 'vue-chartjs';\nexport default {\n  extends: Line,\n  props: ['data'],\n  mounted() {\n    this.renderChart({\n      labels: this.data.map(item => item.country),\n      datasets: [{\n        label: 'Cases',\n        backgroundColor: 'rgba(255, 99, 132, 0.2)',\n        borderColor: 'rgb(255, 99, 132)',\n        data: this.data.map(item => item.timeline.cases[this.selectedDate])\n      }, {\n        label: 'Deaths',\n        backgroundColor: 'rgba(54, 162, 235, 0.2)',\n        borderColor: 'rgb(54, 162, 235)',\n        data: this.data.map(item => item.timeline.deaths[this.selectedDate])\n      }, {\n        label: 'Recovered',\n        backgroundColor: 'rgba(255, 206, 86, 0.2)',\n        borderColor: 'rgb(255, 206, 86)',\n        data: this.data.map(item => item.timeline.recovered[this.selectedDate])\n      }]\n    }, {\n      responsive: true,\n      maintainAspectRatio: false\n    });\n  },\n  watch: {\n    data: function () {\n      this.$data._chart.destroy();\n      this.renderChart({\n        labels: this.data.map(item => item.country),\n        datasets: [{\n          label: 'Cases',\n          backgroundColor: 'rgba(255, 99, 132, 0.2)',\n          borderColor: 'rgb(255, 99, 132)',\n          data: this.data.map(item => item.timeline.cases[this.selectedDate])\n        }, {\n          label: 'Deaths',\n          backgroundColor: 'rgba(54, 162, 235, 0.2)',\n          borderColor: 'rgb(54, 162, 235)',\n          data: this.data.map(item => item.timeline.deaths[this.selectedDate])\n        }, {\n          label: 'Recovered',\n          backgroundColor: 'rgba(255, 206, 86, 0.2)',\n          borderColor: 'rgb(255, 206, 86)',\n          data: this.data.map(item => item.timeline.recovered[this.selectedDate])\n        }]\n      }, {\n        responsive: true,\n        maintainAspectRatio: false\n      });\n    }\n  }\n};","map":{"version":3,"names":["Line","extends","props","mounted","renderChart","labels","data","map","item","co