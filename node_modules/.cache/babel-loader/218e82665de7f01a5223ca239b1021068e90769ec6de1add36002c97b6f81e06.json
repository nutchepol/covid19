{"ast":null,"code":"import { Line } from 'vue-chartjs';\nimport { CategoryScale } from 'chart.js';\nChart.register(CategoryScale);\nexport default {\n  props: ['data', 'selectedDate'],\n  components: {\n    Line\n  },\n  data() {\n    return {\n      data1: {\n        labels: this.data.map(item => item.country),\n        datasets: [{\n          label: 'Cases',\n          backgroundColor: 'rgba(255, 99, 132, 0.2)',\n          borderColor: 'rgb(255, 99, 132)',\n          data: this.data.map(item => item.timeline.cases[this.selectedDate])\n        }, {\n          label: 'Deaths',\n          backgroundColor: 'rgba(54, 162, 235, 0.2)',\n          borderColor: 'rgb(54, 162, 235)',\n          data: this.data.map(item => item.timeline.deaths[this.selectedDate])\n        }, {\n          label: 'Recovered',\n          backgroundColor: 'rgba(255, 206, 86, 0.2)',\n          borderColor: 'rgb(255, 206, 86)',\n          data: this.data.map(item => item.timeline.recovered[this.selectedDate])\n        }]\n      },\n      options: {\n        responsive: true,\n        maintainAspectRatio: false\n      }\n    };\n  }\n};","map":{"version":3,"names":["Line","CategoryScale","Chart","register","props","components","data","data1","labels","map","item","country","datasets","label","backgroundColor","borderColor","timeline","cases","selectedDate","deaths","recovered","options","responsive","maintainAspectRatio"],"sources":["/Users/kgreat/work/KU_project/Vue-covid/visualize-covid19/src/components/ChartCountry.vue"],"sourcesContent":["<template>\n    <div>\n        <h2>Cases by Country</h2>\n        <Line :data=\"data1\" :options='options' />\n    </div>\n</template>\n\n<script>\nimport { Line } from 'vue-chartjs';\nimport { CategoryScale } from 'chart.js';\nChart.register(CategoryScale);\nexport default {\n    props: ['data', 'selectedDate'],\n    components: {\n        Line\n    },\n    data() {\n        return {\n            data1: {\n                labels: this.data.map(item => item.cou